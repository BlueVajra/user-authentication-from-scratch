<% if user != nil %>
  <h1>Welcome <%= user %>!</h1>
  <a href="/logout" alt="Logout">Logout</a>
  <% if admin %>
    <a href="/users" alt="View all users">View all users</a>
  <% end %>
<% else %>
  <h1>Welcome!</h1>
  <a href="/login" alt="Login">Login</a> <a href="/register">Register</a>
<% end %>


