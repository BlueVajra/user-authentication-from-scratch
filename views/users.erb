<section>
  <h2>Users</h2>
  <table class="users">
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>Admin</th>
      <th>Edit</th>
    </tr>
    <% users.each do |user| %>
      <tr>
        <td><%= user[:id] %></td>
        <td><%= user[:user_email] %></td>
        <td><%= user[:administrator] %></td>
        <td class="user"><a href="/user/<%= user[:id] %>">Edit</a></td>
      </tr>
    <% end %>
  </table>
</section>