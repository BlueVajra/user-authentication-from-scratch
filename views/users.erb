<nav>
  <p class="pull-left"><a href="/">Home</a></p>

  <p class="pull-right">Welcome, <%= user_email %> | <a href="/logout">logout</a></p>
</nav>
<section>
  <h1>Users</h1>

  <table class="users">
    <tr>
      <th>ID</th>
      <th>Email</th>
    </tr>
    <% users.each do |user| %>
      <tr>
        <td><%= user[:id] %></td>
        <td><%= user[:user_email] %></td>
        <td class="user"><a href="/user/<%= user[:id] %>">Edit</a></td>
      </tr>
    <% end %>
  </table>
</section>